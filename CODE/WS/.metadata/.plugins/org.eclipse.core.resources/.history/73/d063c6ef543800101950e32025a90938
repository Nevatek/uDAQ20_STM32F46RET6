#ifndef SRC_APPLICATIONLAYER_H_
#define SRC_APPLICATIONLAYER_H_

typedef enum
{
	en_AD7616_IDLE = 0U,
	en_AD7616_START_OF_CONV,
	en_AD7616_STATE_OF_BUSY,
	en_AD7616_READING_CHANNEL,
	en_AD7616_CHANGE_CHANNEL,
	en_AD7616_COMPLETED,
}AD7616_STATE;

void ApplicationLayer_Init(void);
void ApplicationLayer_Exe(void);
#endif /* SRC_APPLICATIONLAYER_H_ */
