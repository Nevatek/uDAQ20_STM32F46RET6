#include "main.h"
#include "Drv_AD7616.h"
#include "ApplicationLayer.h"


void ApplicationLayer_Init(void)
{
	Drv_AD7616_Init();
}

void ApplicationLayer_Exe(void)
{
//	Drv_AD7616_Handler();

	HAL_GPIO_TogglePin(AD7616_CONV_GPIO_Port , AD7616_CONV_Pin);
	Drv_DelayBlocking_ns(1000000);
}
